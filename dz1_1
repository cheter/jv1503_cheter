package dz_1_3_1;

public class Main {

    public static void main(String[] args) {
	byte a = 10;
            System.out.println(a);
        short b = a;
            System.out.println("iz byte B short : ");
            System.out.println(b);

        int c = b;
        System.out.println("iz short B int ");
            System.out.println(c);

        long d = c;
        System.out.println("iz int B long");
            System.out.println(d);

        double e = d;
        System.out.println("iz long B double");
            System.out.println(e);

        char f ='h';
        int g = f;
        System.out.println("iz char B int");
            System.out.println(g);

                float i = g;
        System.out.println("iz int B float");
            System.out.println(i);

        double k = g;
        System.out.println("iz float B double");
            System.out.println(k);

        float j = d;
        System.out.println("iz long B float");
        System.out.println(j);
    }
}
